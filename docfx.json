{
  "metadata": [
    {
      "src": [
        {
          "src": "JsonApiClient/",
          "files": ["JsonApiClient.csproj"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "**/runtimes"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "JsonApiClient",
      "_appTitle": "DotNetJsonApiClient",
      "_enableSearch": true,
      "pdf": true
    }
  }
}