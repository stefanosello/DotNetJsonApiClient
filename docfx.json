{
  "metadata": [
    {
      "src": [
        {
          "src": "JsonApiClient/",
          "files": ["JsonApiClient.csproj"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "**/*.{md,yml,svg}"
        ],
        "exclude": [
          "_site/**",
          "**/runtimes"
        ]
      },
      {
        "files": [
          "api/**.yml"
        ],
        "dest": "api"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "DotNet Json:Api Client",
      "_appTitle": "DotNet Json:Api Client",
      "_enableSearch": true,
      "pdf": false
    }
  }
}